# yaml-language-server: $schema=https://github.com/candlecorp/wick/releases/download/nightly/schema.json
---
name: 'usps'
kind: wick/component@v1
metadata:
  version: '0.0.1'
component:
  kind: wick/component/wasmrs@v1
  ref: build/usps.signed.wasm
  operations:
    - name: greet
      inputs:
        - name: input
          type: string
      outputs:
        - name: output
          type: string
    - name: add
      inputs:
        - name: left
          type: u64
        - name: right
          type: u64
      outputs:
        - name: output
          type: u64
