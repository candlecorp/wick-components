# yaml-language-server: $schema=https://raw.githubusercontent.com/candlecorp/wick/main/crates/wick/wick-config/json-schema/manifest.json
---
name: unzip
kind: wick/component@v1
package:
  registry:
    host: 'registry.candle.dev'
    namespace: 'common'
metadata:
  version: 0.0.1
import: []
component:
  kind: wick/component/wasmrs@v1
  ref: ./build/unzip.signed.wasm
  operations:
    - name: unzip
      inputs:
        - name: input
          type: bytes
      outputs:
        - name: filename
          type: string
        - name: contents
          type: bytes
tests:
  - cases:
      - name: unzip
        operation: unzip
        inputs:
          - name: input
            value: ''
        outputs:
          - name: filename
            value: 'README'
          - name: contents
            value: ''
